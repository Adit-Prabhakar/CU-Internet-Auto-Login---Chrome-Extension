{
  "manifest_version": 3,
  "name": "CU Internet Auto Login",
  "version": "1.1.1",
  "description": "Automatically logs you into Chandigarh University Internet Authentication portal, CUIMS & LMS.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://172.16.2.1/*",
    "https://lms.cuchd.in/*",
    "https://students.cuchd.in/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://172.16.2.1:1000/fgtauth*",
        "https://lms.cuchd.in/*"
      ],
      "js": [
        "content.js"
      ]
    },
    {
      "matches": [
        "https://students.cuchd.in/*"
      ],
      "js": [
        "cuimslogin.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "setup.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Open CU Internet popup"
    }
  }
}